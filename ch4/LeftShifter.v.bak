`include "define.h"

module LeftShifter(Amt_Left, D_in, D_Left);
	
	// how many shifters will be needed
	input [`AMT_WIDTH-1:0] Amt_in;
	// data in
	input [`D_WIDTH-1:0] D_in;
	// data left out
	output reg [`D_WIDTH-1:0] D_Left;
	
	// internal reg variable
	integer Index;
	
	always @* begin
		if(Amt_in[0])
	end
	
endmodule
